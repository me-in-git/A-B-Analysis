   "source": [
    "def proportion_ci(successes: int, total: int, alpha: float = 0.05) -> tuple[float, float]:\n",
    "    if total == 0:\n",
    "        return (math.nan, math.nan)\n",
    "    p = successes / total\n",
    "    z = stats.norm.ppf(1 - alpha / 2)\n",
    "    se = math.sqrt(p * (1 - p) / total)\n",
    "    return (p - z * se, p + z * se)\n",
    "\n",
    "\n",
    "def compare_two_proportions(control: tuple[int, int], treatment: tuple[int, int], alpha: float = 0.05):\n",
    "    c_success, c_total = control\n",
    "    t_success, t_total = treatment\n",
    "    pooled_p = (c_success + t_success) / (c_total + t_total)\n",
    "    se = math.sqrt(pooled_p * (1 - pooled_p) * (1 / c_total + 1 / t_total))\n",
    "    lift = (t_success / t_total) - (c_success / c_total)\n",
    "    z = lift / se\n",
    "    p_value = 2 * (1 - stats.norm.cdf(abs(z)))\n",
    "\n",
    "    se_unpooled = math.sqrt(\n",
    "        (c_success / c_total) * (1 - c_success / c_total) / c_total\n",
    "        + (t_success / t_total) * (1 - t_success / t_total) / t_total\n",
    "    )\n",
    "    z_alpha = stats.norm.ppf(1 - alpha / 2)\n",
    "    ci_lift = (lift - z_alpha * se_unpooled, lift + z_alpha * se_unpooled)\n",
    "\n",
    "    return {\n",
    "        \"lift\": lift,\n",
    "        \"z_stat\": z,\n",
    "        \"p_value\": p_value,\n",
    "        \"ci_lift\": ci_lift,\n",
    "    }\n",
    "\n",
    "c = summary.loc[\"control\"]\n",
    "t = summary.loc[\"treatment\"]\n",
    "\n",
    "result = compare_two_proportions(\n",
    "    control=(int(c.engaged_users), int(c.users)),\n",
    "    treatment=(int(t.engaged_users), int(t.users)),\n",
    ")\n",
    "ci_c = proportion_ci(int(c.engaged_users), int(c.users))\n",
    "ci_t = proportion_ci(int(t.engaged_users), int(t.users))\n",
    "\n",
    "result, ci_c, ci_t\n"
   ]
