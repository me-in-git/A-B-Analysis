   "source": [
    "COL_MAP = {\n",
    "    \"campaign name\": \"campaign_name\",\n",
    "    \"date\": \"date\",\n",
    "    \"spend [usd]\": \"spend_usd\",\n",
    "    \"# of impressions\": \"impressions\",\n",
    "    \"reach\": \"reach\",\n",
    "    \"# of website clicks\": \"clicks\",\n",
    "    \"# of searches\": \"searches\",\n",
    "    \"# of view content\": \"view_content\",\n",
    "    \"# of add to cart\": \"add_to_cart\",\n",
    "    \"# of purchase\": \"purchases\",\n",
    "}\n",
    "\n",
    "def normalize_group(val: str) -> str:\n",
    "    val_l = str(val).lower()\n",
    "    if \"control\" in val_l:\n",
    "        return \"control\"\n",
    "    if \"test\" in val_l or \"treatment\" in val_l or \"variant\" in val_l:\n",
    "        return \"treatment\"\n",
    "    return val_l\n",
    "\n",
    "def read_campaign_csv(path: Path) -> pd.DataFrame:\n",
    "    df = pd.read_csv(path, sep=None, engine=\"python\")\n",
    "    renamed = {}\n",
    "    for c in df.columns:\n",
    "        key = c.strip().lower().replace(\"\\ufeff\", \"\")\n",
    "        renamed[c] = COL_MAP.get(key, key.replace(\" \", \"_\"))\n",
    "    df = df.rename(columns=renamed)\n",
    "\n",
    "    if \"group\" not in df.columns:\n",
    "        if \"campaign_name\" in df.columns:\n",
    "            df[\"group\"] = df[\"campaign_name\"].apply(normalize_group)\n",
    "        else:\n",
    "            df[\"group\"] = normalize_group(path.stem)\n",
    "    else:\n",
    "        df[\"group\"] = df[\"group\"].apply(normalize_group)\n",
    "\n",
    "    if \"date\" in df.columns:\n",
    "        df[\"date\"] = pd.to_datetime(df[\"date\"], dayfirst=True, errors=\"coerce\")\n",
    "\n",
    "    numeric_cols = [\n",
    "        \"spend_usd\",\n",
    "        \"impressions\",\n",
    "        \"reach\",\n",
    "        \"clicks\",\n",
    "        \"searches\",\n",
    "        \"view_content\",\n",
    "        \"add_to_cart\",\n",
    "        \"purchases\",\n",
    "    ]\n",
    "    for col in numeric_cols:\n",
    "        if col in df.columns:\n",
    "            df[col] = pd.to_numeric(df[col], errors=\"coerce\").fillna(0)\n",
    "    return df\n",
    "\n",
    "def load_campaigns(control_path: Path, test_path: Path) -> pd.DataFrame:\n",
    "    df_control = read_campaign_csv(control_path)\n",
    "    df_test = read_campaign_csv(test_path)\n",
    "    combined = pd.concat([df_control, df_test], ignore_index=True)\n",
    "    combined = combined[combined[\"group\"].isin([\"control\", \"treatment\"])]\n",
    "    return combined\n",
    "\n",
    "df = load_campaigns(CONTROL_CSV, TEST_CSV)\n",
    "df.head()\n"
   ]
  },
