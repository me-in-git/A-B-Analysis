    "def summarize(df: pd.DataFrame) -> pd.DataFrame:\n",
    "    grouped = (\n",
    "        df.groupby(\"group\")[ [\"purchases\", \"clicks\", \"reach\", \"spend_usd\"] ]\n",
    "        .sum(min_count=1)\n",
    "        .rename(columns={\"purchases\": \"engaged_users\", \"clicks\": \"users\"})\n",
    "    )\n",
    "    grouped[\"engagement_rate\"] = grouped[\"engaged_users\"] / grouped[\"users\"].replace(0, math.nan)\n",
    "    grouped[\"purchase_rate_per_reach\"] = grouped[\"engaged_users\"] / grouped[\"reach\"].replace(0, math.nan)\n",
    "    return grouped\n",
    "\n",
    "summary = summarize(df)\n",
    "summary\n"
   ]
